{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "typescript-eslint-parser",
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off"
  }
}
